<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.laptop-monitor" path="/org/gnome/shell/extensions/laptop-monitor/">
    <key name="disable-laptop-monitor" type="as">
      <default>['&lt;Super&gt;&lt;Shift&gt;l', '&lt;Alt&gt;F23']</default>
      <summary>Disable Laptop Monitor</summary>
      <description>Keyboard shortcut to turn off the laptop monitor.</description>
    </key>
    <key name="start-wezterm" type="as">
      <default>['&lt;Alt&gt;F24', '&lt;Super&gt;&lt;Shift&gt;w']</default>
      <summary>Start Wezterm</summary>
      <description>Keyboard shortcut to start Wezterm.</description>
    </key>
  </schema>
</schemalist>

